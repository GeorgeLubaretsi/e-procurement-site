<div class="AddCPVGroup">
  <%= render :partial => "user/cpv_create_button" %>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th class="header-5"></th>
      <th class="header-7"></th>
      <th><%=t("Name")%></th>
      <th><%=t("Date Saved")%></th>
  </thead>
  <tbody>
    <% @cpvGroups.each do |cpvGroup| %>
      <tr>
        <td><%= link_to(image_tag("delete-icon.png"), {:controller => "cpv_tree", :action => "deleteCPVTree", :cpvGroup => cpvGroup, :isGlobal => @isGlobal}, :class => "confirm") %></td>
        <td><%= button_to "Edit", {:controller => "cpv_tree", :action => "editCPVTree", :cpvGroup => cpvGroup, :isGlobal => @isGlobal}, :class => "user-button" %></td>
        <td><%= cpvGroup.name %></td>
        <td><%= cpvGroup.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
